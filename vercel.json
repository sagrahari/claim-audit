{
  "framework": "vite",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.py" },
    { "source": "/((?!api/|_next/|assets/|favicon.ico|vite.svg).*)", "destination": "/index.html" }
  ],
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist"
}